; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="OperasiMerahPutih"
config/description="Operasi Merah Putih adalah sebuah game yang mengusung tema patriotisme Indonesia, berlatar belakang perjuangan mempertahankan kedaulatan NKRI dari ancaman musuh Partai Komunis Indonesia."
config/version="1.0.0"
run/main_scene="res://scenes/Menu.tscn"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")
config/icon="res://Logo.png"

[autoload]

SystemVariable="*res://script/globalVariable/SystemVariable.cs"

[display]

window/size/viewport_width=1536
window/size/viewport_height=864
window/size/mode=1
window/stretch/mode="viewport"
window/stretch/scale=0.75

[dotnet]

project/assembly_name="Game Sejarah"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true
